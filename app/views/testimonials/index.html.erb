<div class="banner banner-commun" style="background-image: linear-gradient(-225deg, rgba(0, 0, 0, 0.6) 0%, rgba(0,0,0,0.6) 40%), url(<%= asset_path('testimonial-header.jpg') %>;">
  <div class="banner-content">
    <h1> <%= t(:testimonial_h1) %></h1>
  </div>
</div>
<div class="container">
  <div class="row">
<div class="text-center"><iframe width="853" height="480" src="https://www.youtube.com/embed/ObUcj1pCgrg?rel=0&amp;showinfo=0;autoplay=1" frameborder="0" allowfullscreen></iframe></div>
  <div class="cards">
      <div class="grid">
        <% @testimonials.each do |testimonial| %>
        <div class="<%= important?(testimonial) %>">
          <div class="testi-card">
            <div class="testi-card-content">
              <p>"<%= testimonial.text %>"</p>
            </div>
            <div class="testi-card-name">
              <h3><%= testimonial.author %></h3>
            </div>
          </div>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
