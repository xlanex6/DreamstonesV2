<div class="project-cards">
  <% @projects.each do |project| %>
    <div class="project-card" style="background-image: linear-gradient( -225deg ,rgba(0,0,0,0.4), rgba(0,0,0,0.4) ), url('<%= cl_image_path project.photo.path, height: 189, width: 295, crop: :fill %>');">
      <%= link_to project_path(project) do %>
      <div class="project-title">
        <h3><%= project.title %></h3>
      </div>
      <div class="project-category-img">
        <%= image_tag "#{icon_category(project.category)}-home.png" , class: 'logo-project-type-small'%>
      </div>
      <!-- <div class="project-category-text">
        <%= project.category %>
      </div> -->

    </div>
    <% end %>
  <% end %>
</div>
