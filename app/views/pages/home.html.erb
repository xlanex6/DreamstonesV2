<div class="banner banner-home" style="background-image: linear-gradient(-225deg, rgba(0, 0, 0, 0.4) 0%, rgba(0,0,0,0.4) 0%), url(<%= asset_path('sk2real.jpg') %>);">
  <div class="banner-content text-center">
    <h1><%= t(:baseline)%></h1>
    <p><%= t(:baseline_p)%></p>
    <%= link_to "#{t(:baseline_cta)}", process_path , class: "ds-btn btn-blue"%>
  </div>
</div>


<div class="wrapper-dark-home">
  <div class="row">
    <div class="home-icons">
      <div class="col-xs-12 col-sm-4 col-md-3">
        <div class="home-icon" id="new-build">
          <%= image_tag 'newbuild-home.png'%>
          <h3><%= t(:home_new_build)%></h3>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4 col-md-3">
        <div class="home-icon" id="renovation">
          <%= image_tag 'renovation-home.png'%>
          <h3><%= t(:home_renovation)%></h3>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4 col-md-3">
        <div class="home-icon" id="extension">
          <%= image_tag 'extension-home.png'%>
          <h3><%= t(:home_extension)%></h3>
        </div>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-3">
        <div class="france">
          <%= image_tag 'france.png'%>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="project-cards">
      <% @projects.each do |project| %>
      <%= link_to project_path(project), class: 'prob' do %>
      <div class="col-xs-12 col-sm-6 col-md-4">
        <div class="project-card" style="background-image: linear-gradient( -225deg ,rgba(0,0,0,0.4), rgba(0,0,0,0.4) ), url('<%= cl_image_path project.photo.path, height: 189, width: 295, crop: :fill %>');">
          <div class="project-title">
            <h3><%= project.title %></h3>
          </div>
        </div>
      </div>
      <% end %>
      <% end %>
    </div>
  </div>
</div>

<%= render 'shared/media-partners' %>
<%= render 'shared/simple-process-light' %>
