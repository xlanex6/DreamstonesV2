<% content_for :meta_title, 'Unique Process - Design & Build' %>
<% content_for :meta_description, 'Our unique process made by chartered architects and builders. By following the Preparation - Design - Build process, Dreamstones supports your bespoke project in France' %>
<% content_for :meta_image, asset_path('process-bg.jpg') %>
<% content_for :robots, 'index, follow' %>

<div class="banner banner-process" style="background-image: linear-gradient(-225deg, rgba(0, 0, 0, 0.6) 0%, rgba(0,0,0,0.6) 40%), url(<%= asset_path('process-bg.jpg') %>);">
  <div class="banner-content">
    <h1><%= t(:process_page_h1) %></h1>
  </div>
</div>
<div class="wrapper-dark">
